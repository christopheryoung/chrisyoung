@ARTICLE{ANNAS77-78,
  author = {Annas, Julia},
  title = {How Basic Are Basic Actions?},
  journal = {Proceedings of the Aristotelian Society},
  year = {1977-78},
  volume = { 78},
  abstract = {THE CONCEPT OF A BASIC ACTION HAS BEEN PROMINENT IN RECENT PHILOSOPHY
	OF ACTION. I EXAMINE VARIOUS NOTIONS OF BASICNESS AND ARGUE THAT
	THE ONLY PHILOSOPHICALLY INTERESTING ONE IS THAT OF CAUSAL BASICNESS.
	I THEN EXAMINE ARGUMENTS TO SHOW THAT CAUSALLY BASIC ACTIONS MUST
	BE EITHER BODILY MOVEMENTS OR ELSE ACTS OF WILLING THAT BRING ABOUT
	BODILY MOVEMENTS. IMPORTANT PROBLEMS ARISE FOR BOTH THESE THEORIES,
	AND IT IS ARGUED THAT THE NOTION OF A BASIC ACTION CAN PLAY NO USEFUL
	ROLE IN THE ANALYSIS OR UNDERSTANDING OF ACTION AND AGENCY.},
  keywords = {action, philosophical-anthropology; volition},
  timestamp = {2006.05.08}
}

